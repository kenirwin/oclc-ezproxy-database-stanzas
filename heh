<script type="text/javascript">/*<![CDATA[*/
function getCookie(cname) {
    var name = cname + "=";
    var decodedCookie = decodeURIComponent(document.cookie);
    var ca = decodedCookie.split(';');
    for(var i = 0; i <ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
}
function setCookie(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
    var expires = "expires="+d.toUTCString();
    document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
}
var thisSiteLanguage='';
if (window.location.host == 'oclc-sandbox.mindtouch.us' || window.location.host == 'help.oclc.org') {thisSiteLanguage='en'}
if (window.location.host == 'help-fr.oclc.org' || window.location.host == 'fr-b1887160206b09594.getsmartling.com') {thisSiteLanguage='fr'}
console.log('priorSelection');
console.log(priorSelection);
if (typeof priorSelection == 'undefined') {priorSelection=''}
function goFrench(path) {
   if (window.location.host != 'fr-b1887160206b09594.getsmartling.com' && window.location.host != 'help-fr.oclc.org') {
     setCookie("oclc_helpsite","fr",2);
     window.location='https://help-fr.oclc.org'+path+'?sl=fr';
   }
}
function goEnglish(path) {
    if (window.location.host != 'help.oclc.org' && window.location.host != 'oclc-sandbox.mindtouch.us') {
      setCookie("oclc_helpsite","en",2);
      window.location='https://help.oclc.org'+path+'?sl=en';
    }
}
if (priorSelection!='') {
  setCookie('oclc_helpsite',priorSelection,2);
}
var alang = navigator.languages && navigator.languages[0] || navigator.language || navigator.userLanguage;
if (getCookie("oclc_helpsite") != thisSiteLanguage) {
  if (alang.toLowerCase().includes('fr')) {
      goFrench(window.location.pathname);
  } else {
      goEnglish(window.location.pathname);
  }
}
